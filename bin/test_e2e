#!/usr/bin/env sh
echo "runnint e2e tests"
bin/download-node-image
bin/download-cypress-image
docker-compose -f docker-compose.test_e2e.yml up --build --force-recreate --abort-on-container-exit
docker stop $(docker ps -a -q)
docker rm -f tsks-web-app
docker rm -f tsks-web-test_e2e
docker rmi -f tsks-web-app
docker rmi -f tsks-web-test_e2e
